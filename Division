#Division

#Standard Division - return a float 
print(20/5)
print(60.5/.05)
print(20/3)

#Floor Division - return an integer
print(20//3)
print(20//4)

#Modulus - return float or an integer
print(100%8)
print(100%92)
print(20%2.5)
...
num = 12
if  (num%2) == 0:
    print(f"{num} is even")
else:
    print(f"{num} is odd")
    


